fibers = []
counter = 0
max = 100000

sum = 0

fn callnext() {
    sum = sum + counter
    if(counter < 99999) {
        counter++
        fibers[counter].run()
    }
}

s = callnext@0

start = clock()

for(i in range(max)) {
    fibers[i] = fiber(s)
}

fibers[0].run()

end = clock()

print(sum, "\nelapsed: ", (end-start)/clocks_per_sec, "\n")
