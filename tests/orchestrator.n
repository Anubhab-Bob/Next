modules = [prepost, iftest, primitivetest, classtest, importest, opmethod,
            subscriptest, arraytest, exceptiontest, rerrtest, typetest]

desc = ["Pre and post increment/decrements", "Short circuits",
        "Primitive method calls", "Classes", "Imports",
        "Operator methods", "Subscript getter and setter",
        "Arrays", "Exceptions", "Runtime exceptions",
        "Type builtins"]

i = 0
while(i < modules.size()) {
    print("[Testing] ", desc[i], " -> ")
